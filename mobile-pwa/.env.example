# Auth0 Configuration for Enterprise SSO
VITE_AUTH0_DOMAIN=your-domain.auth0.com
VITE_AUTH0_CLIENT_ID=your-client-id
VITE_AUTH0_AUDIENCE=https://leanvibe-agent-hive

# Firebase Configuration for Push Notifications
VITE_FIREBASE_API_KEY=your-firebase-api-key
VITE_FIREBASE_AUTH_DOMAIN=your-project.firebaseapp.com
VITE_FIREBASE_PROJECT_ID=your-project-id
VITE_FIREBASE_STORAGE_BUCKET=your-project.appspot.com
VITE_FIREBASE_MESSAGING_SENDER_ID=123456789
VITE_FIREBASE_APP_ID=1:123456789:web:abcd123456
VITE_FIREBASE_MEASUREMENT_ID=G-ABCD123456

# Backend API Configuration
VITE_API_BASE_URL=http://localhost:8000
VITE_WS_BASE_URL=ws://localhost:8000

# PWA Configuration
VITE_PWA_NAME=LeanVibe Agent Hive
VITE_PWA_SHORT_NAME=Agent Hive
VITE_PWA_DESCRIPTION=Enterprise Autonomous Development Platform

# Security Configuration
VITE_ENABLE_SECURITY_LOGGING=true
VITE_SESSION_TIMEOUT_MINUTES=30
VITE_TOKEN_REFRESH_MINUTES=15

# Development Configuration
VITE_DEBUG_MODE=false
VITE_LOG_LEVEL=info