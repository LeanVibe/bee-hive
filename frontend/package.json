{
  "name": "leanvibe-observability-dashboard",
  "version": "2.0.0",
  "type": "module",
  "description": "Real-time observability dashboard for LeanVibe Agent Hive",
  "author": "LeanVibe Team",
  "license": "MIT",
  "scripts": {
    "dev": "vite",
    "build": "vue-tsc && vite build",
    "build-no-check": "vite build",
    "preview": "vite preview",
    "type-check": "vue-tsc --noEmit",
    "lint": "eslint . --ext .vue,.js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts --fix --ignore-path .gitignore",
    "format": "prettier --write src/",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "test:coverage": "vitest --coverage",
    "test:e2e": "playwright test",
    "test:e2e:ui": "playwright test --ui",
    "test:e2e:headed": "playwright test --headed",
    "test:e2e:debug": "playwright test --debug",
    "test:e2e:workflows": "playwright test tests/e2e/workflows",
    "test:e2e:pwa": "playwright test tests/e2e/pwa",
    "test:e2e:performance": "playwright test tests/e2e/performance",
    "test:e2e:accessibility": "playwright test tests/e2e/accessibility",
    "test:e2e:report": "playwright show-report",
    "test:pyramid": "npm run test && npm run test:e2e",
    "test:pyramid:ci": "npm run test:coverage && npm run test:e2e -- --reporter=junit",
    "playwright:install": "playwright install",
    "playwright:codegen": "playwright codegen localhost:5173"
  },
  "dependencies": {
    "@headlessui/vue": "^1.7.16",
    "@heroicons/vue": "^2.0.18",
    "@types/d3": "^7.4.3",
    "autoprefixer": "^10.4.16",
    "chart.js": "^4.4.1",
    "chartjs-adapter-date-fns": "^3.0.0",
    "d3": "^7.9.0",
    "d3-drag": "^3.0.0",
    "d3-force": "^3.0.0",
    "d3-scale": "^4.0.2",
    "d3-selection": "^3.0.0",
    "d3-time-format": "^4.1.0",
    "d3-zoom": "^3.0.0",
    "date-fns": "^3.0.6",
    "lodash-es": "^4.17.21",
    "pinia": "^2.1.7",
    "postcss": "^8.4.32",
    "tailwindcss": "^3.4.0",
    "vue": "^3.4.0",
    "vue-chartjs": "^5.3.0",
    "vue-router": "^4.2.5"
  },
  "devDependencies": {
    "@playwright/test": "^1.55.0",
    "@types/lodash-es": "^4.17.12",
    "@types/node": "^20.10.0",
    "@vitejs/plugin-vue": "^5.0.0",
    "@vitest/ui": "^1.1.0",
    "@vue/eslint-config-prettier": "^9.0.0",
    "@vue/eslint-config-typescript": "^12.0.0",
    "@vue/test-utils": "^2.4.3",
    "@vue/tsconfig": "^0.5.0",
    "c8": "^9.0.0",
    "eslint": "^8.56.0",
    "eslint-plugin-vue": "^9.19.2",
    "jsdom": "^23.0.1",
    "playwright": "^1.55.0",
    "prettier": "^3.1.1",
    "typescript": "~5.3.0",
    "vite": "^5.0.10",
    "vitest": "^1.1.0",
    "vue-tsc": "^1.8.25"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
