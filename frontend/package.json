{
  "name": "leanvibe-observability-dashboard",
  "version": "2.0.0",
  "type": "module",
  "description": "Real-time observability dashboard for LeanVibe Agent Hive",
  "author": "LeanVibe Team",
  "license": "MIT",
  "scripts": {
    "dev": "vite",
    "build": "vue-tsc && vite build",
    "preview": "vite preview",
    "type-check": "vue-tsc --noEmit",
    "lint": "eslint . --ext .vue,.js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts --fix --ignore-path .gitignore",
    "format": "prettier --write src/",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "test:coverage": "vitest --coverage"
  },
  "dependencies": {
    "vue": "^3.4.0",
    "vue-router": "^4.2.5",
    "pinia": "^2.1.7",
    "chart.js": "^4.4.1",
    "vue-chartjs": "^5.3.0",
    "date-fns": "^3.0.6",
    "@headlessui/vue": "^1.7.16",
    "@heroicons/vue": "^2.0.18",
    "tailwindcss": "^3.4.0",
    "autoprefixer": "^10.4.16",
    "postcss": "^8.4.32"
  },
  "devDependencies": {
    "@vitejs/plugin-vue": "^5.0.0",
    "@vue/tsconfig": "^0.5.0",
    "typescript": "~5.3.0",
    "vue-tsc": "^1.8.25",
    "vite": "^5.0.10",
    "@types/node": "^20.10.0",
    "eslint": "^8.56.0",
    "@vue/eslint-config-typescript": "^12.0.0",
    "@vue/eslint-config-prettier": "^9.0.0",
    "eslint-plugin-vue": "^9.19.2",
    "prettier": "^3.1.1",
    "vitest": "^1.1.0",
    "jsdom": "^23.0.1",
    "@vue/test-utils": "^2.4.3",
    "@vitest/ui": "^1.1.0",
    "c8": "^9.0.0"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}