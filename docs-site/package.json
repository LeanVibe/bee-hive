{
  "name": "leanvibe-docs-site",
  "version": "1.0.0",
  "type": "module",
  "description": "Modern VitePress documentation site for LeanVibe Agent Hive",
  "author": "LeanVibe Team",
  "license": "MIT",
  "scripts": {
    "dev": "vitepress dev",
    "build": "vitepress build",
    "preview": "vitepress preview",
    "build:search": "npm run build && node .vitepress/build-search-index.js",
    "deploy": "npm run build && npm run deploy:gh-pages",
    "deploy:gh-pages": "gh-pages -d .vitepress/dist",
    "lint": "eslint .vitepress --ext .ts,.vue --fix",
    "format": "prettier --write '.vitepress/**/*.{ts,vue,css}'",
    "check-links": "node .vitepress/check-links.js",
    "generate-api": "node .vitepress/generate-api-docs.js"
  },
  "dependencies": {
    "vitepress": "^1.0.0-rc.35",
    "vue": "^3.4.0",
    "algoliasearch": "^4.20.0",
    "@algolia/autocomplete-js": "^1.9.3",
    "fuse.js": "^7.0.0",
    "markdown-it": "^14.0.0",
    "markdown-it-anchor": "^8.6.7",
    "markdown-it-container": "^4.0.0",
    "markdown-it-attrs": "^4.1.6",
    "markdown-it-footnote": "^4.0.0",
    "prismjs": "^1.29.0",
    "date-fns": "^3.6.0",
    "lucide-vue-next": "^0.300.0"
  },
  "devDependencies": {
    "@types/node": "^20.10.0",
    "@typescript-eslint/eslint-plugin": "^6.15.0",
    "@typescript-eslint/parser": "^6.15.0",
    "eslint": "^8.56.0",
    "eslint-plugin-vue": "^9.19.2",
    "prettier": "^3.1.1",
    "typescript": "^5.3.0",
    "gh-pages": "^6.1.0",
    "fast-glob": "^3.3.2",
    "gray-matter": "^4.0.3",
    "feed": "^4.2.2",
    "sitemap": "^7.1.1"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "keywords": [
    "documentation",
    "vitepress",
    "autonomous-development",
    "ai-agents",
    "developer-tools",
    "enterprise-software"
  ]
}