            "custom": {
              "axisLabel": "",
              "drawStyle": "line",
              "fillOpacity": 20,
              "lineWidth": 2,
              "pointSize": 5,
              "showPoints": "never"
            },
            "unit": "currencyUSD"
          },
          "overrides": [
            {
              "matcher": {
                "id": "byName",
                "options": "Customer Impact Score"
              },
              "properties": [
                {
                  "id": "unit",
                  "value": "short"
                },
                {
                  "id": "custom.axisPlacement",
                  "value": "right"
                }
              ]
            }
          ]
        },
        "gridPos": {"h": 8, "w": 24, "x": 0, "y": 28}
      },
      {
        "id": 11,
        "title": "🚨 Predictive Alert Status",
        "type": "table",
        "targets": [
          {
            "expr": "ALERTS{alertname=~\"Predictive.*|Intelligent.*|Capacity.*|Business.*\"}",
            "legendFormat": "",
            "refId": "A",
            "instant": true
          }
        ],
        "fieldConfig": {
          "defaults": {
            "custom": {
              "align": "auto",
              "displayMode": "auto"
            },
            "mappings": [],
            "thresholds": {
              "mode": "absolute",
              "steps": [
                {"color": "green", "value": null},
                {"color": "red", "value": 80}
              ]
            }
          },
          "overrides": [
            {
              "matcher": {
                "id": "byName",
                "options": "alertname"
              },
              "properties": [
                {
                  "id": "custom.width",
                  "value": 250
                }
              ]
            },
            {
              "matcher": {
                "id": "byName",
                "options": "severity"
              },
              "properties": [
                {
                  "id": "custom.displayMode",
                  "value": "color-background"
                },
                {
                  "id": "mappings",
                  "value": [
                    {"options": {"critical": {"color": "red", "index": 0}}},
                    {"options": {"high": {"color": "orange", "index": 1}}},
                    {"options": {"warning": {"color": "yellow", "index": 2}}},
                    {"options": {"info": {"color": "blue", "index": 3}}}
                  ]
                }
              ]
            }
          ]
        },
        "options": {
          "showHeader": true,
          "sortBy": [
            {
              "desc": true,
              "displayName": "severity"
            }
          ]
        },
        "transformations": [
          {
            "id": "organize",
            "options": {
              "excludeByName": {
                "__name__": true,
                "job": true,
                "instance": true
              },
              "indexByName": {},
              "renameByName": {
                "alertname": "Alert Name",
                "severity": "Severity",
                "summary": "Summary",
                "component": "Component"
              }
            }
          }
        ],
        "gridPos": {"h": 8, "w": 24, "x": 0, "y": 36}
      },
      {
