{
  "dashboard": {
    "id": null,
    "title": "Epic F: Predictive Analytics & AI Intelligence",
    "tags": ["epic-f", "predictive", "ai", "observability"],
    "style": "dark",
    "timezone": "browser",
    "editable": true,
    "graphTooltip": 1,
    "time": {
      "from": "now-3h",
      "to": "now"
    },
    "timepicker": {
      "refresh_intervals": ["10s", "30s", "1m", "5m", "15m", "30m", "1h", "2h", "1d"],
      "time_options": ["5m", "15m", "1h", "6h", "12h", "24h", "2d", "7d", "30d"]
    },
    "refresh": "30s",
    "version": 1,
    "panels": [
      {
        "id": 1,
        "title": "🔮 AI-Powered Performance Predictions",
        "type": "stat",
        "targets": [
          {
            "expr": "leanvibe:overall_system_risk_score",
            "legendFormat": "Risk Score",
            "refId": "A"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "color": {
              "mode": "thresholds"
            },
            "thresholds": {
              "steps": [
                {"color": "green", "value": null},
                {"color": "yellow", "value": 0.3},
                {"color": "orange", "value": 0.6},
                {"color": "red", "value": 0.8}
              ]
            },
            "unit": "percentunit",
            "min": 0,
            "max": 1
          }
        },
        "options": {
          "orientation": "auto",
          "reduceOptions": {
            "values": false,
            "calcs": ["lastNotNull"],
            "fields": ""
          },
          "textMode": "value_and_name"
        },
        "gridPos": {"h": 4, "w": 6, "x": 0, "y": 0}
      },
      {
        "id": 2,
        "title": "🎯 Prediction Accuracy",
        "type": "stat",
        "targets": [
          {
            "expr": "leanvibe:prediction_accuracy_score",
            "legendFormat": "Accuracy %",
            "refId": "A"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "color": {
              "mode": "thresholds"
            },
            "thresholds": {
              "steps": [
                {"color": "red", "value": null},
                {"color": "yellow", "value": 70},
                {"color": "green", "value": 85}
              ]
            },
            "unit": "percent",
            "min": 0,
            "max": 100
          }
        },
        "gridPos": {"h": 4, "w": 6, "x": 6, "y": 0}
      },
      {
        "id": 3,
        "title": "⚡ Anomalies Detected (24h)",
        "type": "stat",
        "targets": [
          {
            "expr": "sum(leanvibe:anomaly_frequency_1h)",
            "legendFormat": "Anomalies",
            "refId": "A"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "color": {
              "mode": "thresholds"
            },
            "thresholds": {
              "steps": [
                {"color": "green", "value": null},
                {"color": "yellow", "value": 5},
                {"color": "orange", "value": 15},
                {"color": "red", "value": 30}
              ]
            },
            "unit": "short"
          }
        },
        "gridPos": {"h": 4, "w": 6, "x": 12, "y": 0}
      },
      {
        "id": 4,
        "title": "💰 Business Impact Score",
        "type": "stat",
        "targets": [
          {
            "expr": "leanvibe:business_health_score",
            "legendFormat": "Health Score",
            "refId": "A"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "color": {
              "mode": "thresholds"
            },
            "thresholds": {
              "steps": [
                {"color": "red", "value": null},
                {"color": "orange", "value": 60},
                {"color": "yellow", "value": 80},
                {"color": "green", "value": 90}
              ]
            },
            "unit": "short",
            "min": 0,
            "max": 100
          }
        },
        "gridPos": {"h": 4, "w": 6, "x": 18, "y": 0}
      },
      {
        "id": 5,
        "title": "📈 Performance Predictions vs Reality",
        "type": "timeseries",
        "targets": [
          {
            "expr": "leanvibe:performance_trend_5m",
            "legendFormat": "Actual Response Time",
            "refId": "A"
          },
          {
            "expr": "predict_linear(leanvibe:performance_trend_5m[15m], 1800)",
            "legendFormat": "Predicted (30min)",
            "refId": "B"
          },
          {
            "expr": "leanvibe:adaptive_threshold_upper",
            "legendFormat": "Adaptive Threshold",
            "refId": "C"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "color": {
              "mode": "palette-classic"
            },
            "custom": {
              "axisLabel": "",
              "axisPlacement": "auto",
              "barAlignment": 0,
              "drawStyle": "line",
              "fillOpacity": 10,
              "gradientMode": "none",
              "hideFrom": {
                "legend": false,
                "tooltip": false,
                "vis": false
              },
              "lineInterpolation": "linear",
              "lineWidth": 2,
              "pointSize": 5,
              "scaleDistribution": {
                "type": "linear"
              },
              "showPoints": "never",
              "spanNulls": false,
              "stacking": {
                "group": "A",
                "mode": "none"
              },
              "thresholdsStyle": {
                "mode": "off"
              }
            },
            "unit": "s",
            "min": 0
          },
          "overrides": [
            {
              "matcher": {
                "id": "byName",
                "options": "Predicted (30min)"
              },
              "properties": [
                {
                  "id": "custom.drawStyle",
                  "value": "line"
                },
                {
                  "id": "custom.lineStyle",
                  "value": {
                    "dash": [10, 10],
                    "fill": "dash"
                  }
                }
              ]
            }
          ]
        },
        "options": {
          "legend": {
            "calcs": [],
            "displayMode": "list",
            "placement": "bottom"
          },
          "tooltip": {
            "mode": "single"
          }
        },
        "gridPos": {"h": 8, "w": 12, "x": 0, "y": 4}
      },
      {
        "id": 6,
        "title": "🧠 Anomaly Detection Intelligence",
        "type": "timeseries",
        "targets": [
          {
            "expr": "leanvibe:response_time_zscore",
            "legendFormat": "Response Time Z-Score",
            "refId": "A"
          },
          {
            "expr": "leanvibe:error_rate_zscore",
            "legendFormat": "Error Rate Z-Score",
            "refId": "B"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "color": {
              "mode": "palette-classic"
            },
            "custom": {
              "axisLabel": "Standard Deviations",
              "drawStyle": "line",
              "fillOpacity": 20,
              "lineWidth": 2,
              "pointSize": 5,
              "showPoints": "never",
              "thresholdsStyle": {
                "mode": "area"
              }
            },
            "thresholds": {
              "steps": [
                {"color": "transparent", "value": null},
                {"color": "orange", "value": 2},
                {"color": "red", "value": 3}
              ]
            },
            "unit": "short"
          }
        },
        "gridPos": {"h": 8, "w": 12, "x": 12, "y": 4}
      },
      {
        "id": 7,
        "title": "🔄 Capacity Planning Intelligence",
        "type": "timeseries",
        "targets": [
          {
            "expr": "leanvibe:cpu_utilization_trend",
            "legendFormat": "CPU Predicted (30m)",
            "refId": "A"
          },
          {
            "expr": "cpu_usage_percent",
            "legendFormat": "CPU Current",
            "refId": "B"
          },
          {
            "expr": "leanvibe:memory_utilization_trend",
            "legendFormat": "Memory Predicted (30m)",
            "refId": "C"
          },
          {
            "expr": "memory_usage_percent",
            "legendFormat": "Memory Current",
            "refId": "D"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "color": {
              "mode": "palette-classic"
            },
            "custom": {
              "axisLabel": "Utilization %",
              "drawStyle": "line",
              "fillOpacity": 10,
              "lineWidth": 2,
              "thresholdsStyle": {
                "mode": "area"
              }
            },
            "thresholds": {
              "steps": [
                {"color": "transparent", "value": null},
                {"color": "yellow", "value": 70},
                {"color": "orange", "value": 80},
                {"color": "red", "value": 90}
              ]
            },
            "unit": "percent",
            "min": 0,
            "max": 100
          }
        },
        "gridPos": {"h": 8, "w": 24, "x": 0, "y": 12}
      },
      {
        "id": 8,
        "title": "💼 Business Intelligence Correlation Matrix",
        "type": "heatmap",
        "targets": [
          {
            "expr": "leanvibe:technical_business_correlation",
            "legendFormat": "Tech-Business Correlation",
            "refId": "A"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "custom": {
              "hideFrom": {
                "legend": false,
                "tooltip": false,
                "vis": false
              },
              "scaleDistribution": {
                "type": "linear"
              }
            },
            "color": {
              "mode": "spectrum",
              "scheme": "RdYlGn"
            },
            "unit": "short"
          }
        },
        "options": {
          "calculate": false,
          "cellGap": 2,
          "cellValues": {},
          "color": {
            "exponent": 0.5,
            "fill": "dark-orange",
            "mode": "spectrum",
            "reverse": false,
            "scale": "exponential",
            "scheme": "RdYlGn",
            "steps": 64
          },
          "exemplars": {
            "color": "rgba(255,0,255,0.7)"
          },
          "filterValues": {
            "le": 1e-9
          },
          "legend": {
            "show": true
          },
          "rowsFrame": {
            "layout": "auto"
          },
          "tooltip": {
            "show": true,
            "yHistogram": false
          },
          "yAxis": {
            "axisPlacement": "left",
            "reverse": false,
            "unit": "short"
          }
        },
        "gridPos": {"h": 8, "w": 12, "x": 0, "y": 20}
      },
      {
        "id": 9,
        "title": "🎢 ML Model Performance Tracking",
        "type": "timeseries",
        "targets": [
          {
            "expr": "leanvibe:prediction_model_accuracy",
            "legendFormat": "Prediction Accuracy",
            "refId": "A"
          },
          {
            "expr": "leanvibe:anomaly_detection_precision",
            "legendFormat": "Anomaly Detection Precision",
            "refId": "B"
          },
          {
            "expr": "leanvibe:anomaly_detection_recall",
            "legendFormat": "Anomaly Detection Recall",
            "refId": "C"
          },
          {
            "expr": "leanvibe:feature_stability_score",
            "legendFormat": "Feature Stability",
            "refId": "D"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "color": {
              "mode": "palette-classic"
            },
            "custom": {
              "axisLabel": "Score",
              "drawStyle": "line",
              "fillOpacity": 15,
              "lineWidth": 2,
              "pointSize": 5,
              "showPoints": "auto",
              "thresholdsStyle": {
                "mode": "line"
              }
            },
            "thresholds": {
              "steps": [
                {"color": "transparent", "value": null},
                {"color": "red", "value": 0.7},
                {"color": "yellow", "value": 0.8},
                {"color": "green", "value": 0.9}
              ]
            },
            "unit": "percentunit",
            "min": 0,
            "max": 1
          }
        },
        "gridPos": {"h": 8, "w": 12, "x": 12, "y": 20}
      },
      {
        "id": 10,
        "title": "💵 Revenue Impact Analysis",
        "type": "timeseries",
        "targets": [
          {
            "expr": "leanvibe:estimated_revenue_impact_per_minute",
            "legendFormat": "Revenue Impact ($/min)",
            "refId": "A"
          },
          {
            "expr": "leanvibe:customer_impact_score",
            "legendFormat": "Customer Impact Score",
            "refId": "B"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "color": {
              "mode": "palette-classic"
