      {
        "id": 13,
        "title": "🔮 Capacity Time-to-Threshold Predictions",
        "type": "stat",
        "targets": [
          {
            "expr": "leanvibe:capacity_time_to_threshold",
            "legendFormat": "CPU Threshold (hours)",
            "refId": "A"
          },
          {
            "expr": "predict_linear(memory_usage_percent[30m], 3600) > 85",
            "legendFormat": "Memory Alert Predicted",
            "refId": "B"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "color": {
              "mode": "thresholds"
            },
            "mappings": [
              {
                "options": {
                  "0": {
                    "color": "red",
                    "index": 0,
                    "text": "IMMEDIATE"
                  }
                },
                "type": "value"
              }
            ],
            "thresholds": {
              "mode": "absolute",
              "steps": [
                {"color": "red", "value": null},
                {"color": "orange", "value": 2},
                {"color": "yellow", "value": 8},
                {"color": "green", "value": 24}
              ]
            },
            "unit": "h"
          }
        },
        "options": {
          "colorMode": "background",
          "graphMode": "area",
          "justifyMode": "center",
          "orientation": "auto",
          "reduceOptions": {
            "calcs": ["lastNotNull"],
            "fields": "",
            "values": false
          },
          "text": {},
          "textMode": "value_and_name"
        },
        "gridPos": {"h": 8, "w": 12, "x": 12, "y": 44}
      }
    ],
    "annotations": {
      "list": [
        {
          "builtIn": 1,
          "datasource": "-- Grafana --",
          "enable": true,
          "hide": true,
          "iconColor": "rgba(0, 211, 255, 1)",
          "name": "Annotations & Alerts",
          "type": "dashboard"
        },
        {
          "datasource": "Prometheus",
          "enable": true,
          "expr": "ALERTS{alertname=~\"Predictive.*|Intelligent.*\"}",
          "iconColor": "red",
          "name": "Predictive Alerts",
          "showIn": 0,
          "step": "60s",
          "tagKeys": "alertname,severity",
          "textFormat": "{{alertname}}: {{summary}}",
          "titleFormat": "AI Alert"
        }
      ]
    },
    "templating": {
      "list": [
        {
          "current": {
            "selected": false,
            "text": "All",
            "value": "$__all"
          },
          "datasource": "Prometheus",
          "definition": "label_values(leanvibe_predictions_total, component)",
          "hide": 0,
          "includeAll": true,
          "multi": false,
          "name": "component",
          "options": [],
          "query": {
            "query": "label_values(leanvibe_predictions_total, component)",
            "refId": "StandardVariableQuery"
          },
          "refresh": 1,
          "regex": "",
          "skipUrlSync": false,
          "sort": 0,
          "type": "query"
        }
      ]
    }
